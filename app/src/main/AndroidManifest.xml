<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectitdiv.quickmath">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:configChanges="locale|orientation"
        android:theme="@style/AppTheme">
        <activity android:name=".GuestStageActivity"
            android:parentActivityName=".LoginActivity"/>
        <activity android:name=".MemberStageActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingActivity"
            android:parentActivityName=".GuestStageActivity"/>
        <activity android:name=".ProfileActivity"
            android:parentActivityName=".SettingActivity"/>
        <activity android:name=".LanguageActivity"
            android:parentActivityName=".SettingActivity"></activity>
        <service
            android:name=".BackgroundSoundService"
            android:enabled="true"
            android:exported="true"></service>
    </application>
</manifest>